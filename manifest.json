{
  "manifest_version": 2,
  "name": "Syncpad for Simplenote",
  "version": "2.0.0.1",
  "description": "__MSG_description__",
  "default_locale": "en",
  "icons": {
    "96": "images/icon_96.png",
    "128": "images/icon_128.png",
    "16": "images/icon_16.png",
    "19": "images/icon_24.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png"
  },
  "background": {
    "scripts": ["javascripts/lib/jquery-1.6.2.min.js",
                "javascripts/lib/quicksilver.js",
                "javascripts/lib/webtoolkit.base64.js",
                "javascripts/common.js",
                "javascripts/simplenoteSM.js",
                "javascripts/simplenoteAPI.js",
                "javascripts/simplenoteLS.js",
                "javascripts/simplenoteCM.js",
                "javascripts/simplenoteDB.js",
                "javascripts/simplenoteBG.js"
               ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "images/icon_24.png",
    "default_popup": "popup.html",
    "default_title": "Open Syncpad"
  },
  "permissions": [
    "https://simple-note.appspot.com/",
    "contextMenus",
    "tabs"
  ]
}
